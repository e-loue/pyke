# fc_forall.krb

test1
    foreach
        family.child_of($child, $father, $mother)
        forall
            family.child_of($_, $father, $mother2)
        require
            check $mother == $mother2
        forall
            family.child_of($_, $father2, $mother)
        require
            check $father == $father2
    assert
        python print $child, "has no step brothers or sisters"
